{
  "name": "muhle-lab-web",
  "version": "2.0.0",
  "description": "Muhle Lab website - A TinaCMS-powered research lab website template",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && next build",
    "start": "tinacms build && next start",
    "export": "next build",
    "lint": "echo 'Linting requires ESLint v8 - skipping' && exit 0",
    "type-check": "tsc --noEmit",
    "test": "npm run type-check",
    "test:ci": "npm run type-check"
  },
  "dependencies": {
    "@tinacms/cli": "^1.6.8",
    "clsx": "^2.1.1",
    "next": "^14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swiper": "^11.1.14",
    "tinacms": "^2.2.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidfrivas/muhle-lab-web"
  },
  "keywords": [
    "research-lab",
    "tinacms",
    "nextjs",
    "academic-website",
    "template"
  ],
  "author": "Muhle Lab",
  "license": "MIT"
}
